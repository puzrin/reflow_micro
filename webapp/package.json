{
  "name": "reflow_mock",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write src/",
    "gen:proto:ts": "protoc --ts_proto_out=./src/proto/generated --ts_proto_opt=esModuleInterop=true --proto_path=./src/proto types.proto",
    "gen:proto:data": "node --import=tsimp/import ./src/proto/gen_defaults.ts",
    "gen:proto": "run-p gen:proto:ts gen:proto:data",
    "test": "run-p lint type-check"
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.2.1",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@vueuse/core": "^11.1.0",
    "d3": "^7.9.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "protobufjs": "^7.2.4",
    "vue": "^3.5.12",
    "vue-draggable-plus": "^0.5.3",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tsconfig/node20": "^20.1.4",
    "@types/d3": "^7.4.3",
    "@types/google-protobuf": "^3.15.7",
    "@types/node": "^20.17.6",
    "@types/web-bluetooth": "^0.0.20",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "git-remote-origin-url": "^4.0.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "protoc": "^1.1.3",
    "tailwindcss": "^3.4.13",
    "ts-proto": "^2.2.5",
    "tsimp": "^2.0.12",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.10"
  }
}
