{
  "name": "reflow_mock",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "gen:proto:ts": "protoc --ts_proto_out=./src/proto/generated --ts_proto_opt=esModuleInterop=true --proto_path=./src/proto types.proto",
    "gen:proto": "npm run gen:proto:ts",
    "build-validator-schema": "ts-json-schema-generator --path src/device/heater_config.ts --type ProfilesData --out src/device/profiles_schema.json"
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.2.1",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@vueuse/core": "^11.1.0",
    "ajv": "^8.17.1",
    "d3": "^7.9.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "protobufjs": "^7.2.4",
    "vue": "^3.5.12",
    "vue-draggable-plus": "^0.5.3",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tailwindcss/forms": "^0.5.9",
    "@tsconfig/node18": "^18.2.4",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.7.5",
    "@types/google-protobuf": "^3.15.7",
    "@types/web-bluetooth": "^0.0.20",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "git-remote-origin-url": "^4.0.0",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "protoc": "^1.1.3",
    "tailwindcss": "^3.4.13",
    "ts-json-schema-generator": "^2.3.0",
    "ts-proto": "^2.2.5",
    "typescript": "~5.6.3",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  }
}
